pipeline {
    agent any

    stages {
        stage('stage1') {
            steps {
                script {
                    // myplayground
                    var1=10
                    echo 'Hello, welcome to Jenkins PAC'
                    
                }
            }
        }
        stage('stage2') {
            steps {
                script {
                    // myplayground
                    var1=10
               
                    println "hellow here is your variable value ${var1}"
                }
            }
        }
    }
}
